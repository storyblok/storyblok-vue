{
  "name": "@storyblok/vue",
  "version": "0.0.1",
  "description": "Storyblok directive for get editable elements.",
  "main": "./dist/storyblok-vue.js",
  "module": "./dist/storyblok-vue.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/storyblok-vue.mjs",
      "require": "./dist/storyblok-vue.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build && vue-tsc --declaration --emitDeclarationOnly",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "jest __tests__",
    "test:e2e": "cypress run --component",
    "test:e2e-watch": "cypress open --component",
    "prepublishOnly": "npm run build && cp ../README.md ./"
  },
  "dependencies": {
    "@storyblok/js": "^3.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@cypress/vite-dev-server": "^5.0.7",
    "@cypress/vue": "^6.0.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/babel-preset-app": "^5.0.8",
    "@vue/test-utils": "2.4.5",
    "@vue/tsconfig": "^0.1.3",
    "@vue/vue3-jest": "^29.2.6",
    "babel-jest": "^29.4.3",
    "cypress": "^13.7.2",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-jest": "^27.9.0",
    "jest": "^29.6.4",
    "typescript": "^4.9.5",
    "vite": "^5.2.7",
    "vue": "^3.4.21",
    "vue-tsc": "^1.8.0"
  },
  "babel": {
    "presets": [
      "@vue/babel-preset-app"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.vue$": "@vue/vue3-jest"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/storyblok/storyblok-vue"
  },
  "keywords": [
    "vue",
    "storyblok"
  ],
  "author": "Alexander Feiglstorfer",
  "bugs": {
    "url": "https://github.com/storyblok/storyblok-vue/issues"
  },
  "homepage": "https://github.com/storyblok/storyblok-vue",
  "release": {
    "branches": [
      "main",
      {
        "name": "next",
        "prerelease": true
      },
      {
        "name": "beta",
        "prerelease": true
      }
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
